{
    "data_path": "data",
    "output_path": "output",
    "preprocess": {
        "start_date": "2012-05-01",
        "end_date": "2022-05-01",
        "save_path": "stock_data",
        "window": 20,
        "prediction_day": 5,
        "percentage": 0.05
    },
    "train": {
        "mav_line": false,
        "volume": false,
        "is_binary": true,
        "equalize": true,
        "random_seed": 42,
        "split_ratio": {
            "train": 0.8,
            "valid": 0.1,
            "test": 0.1
        },
        "image_size": 224,
        "batch_size": 64,
        "model": "vgg16",
        "pretrained": true,
        "optimizer": "Adam",
        "lr_scheduler": "Exponential",
        "lr_gamma": 0.998,
        "lr": 1e-4,
        "weight_decay": 1e-3,
        "num_epoch": 50
    }
}