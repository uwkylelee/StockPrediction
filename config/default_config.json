{
    "data_path": "data",
    "output_path": "output",
    "preprocess": {
        "start_date": "2012-05-01",
        "end_date": "2022-05-01",
        "window": 20
    },
    "train": {
        "window": 20,
        "prediction_day": 1,
        "percentage": 0.04,
        "mav_line": false,
        "volume": false,
        "is_binary": true,
        "equalize": true,
        "random_seed": 42,
        "split_ratio": {
            "train": 0.8,
            "valid": 0.1,
            "test": 0.1
        },
        "image_size": 224,
        "batch_size": 128,
        "model": "vgg16_bn",
        "pretrained": true,
        "optimizer": "Adam",
        "lr_scheduler": "Exponential",
        "lr_gamma": 0.98,
        "lr": 0.00008,
        "weight_decay": 0,
        "num_epoch": 50
    }
}